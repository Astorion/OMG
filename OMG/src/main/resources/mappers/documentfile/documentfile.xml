<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.omg.documentfile">


	<insert id="doInsert"  parameterType="DocumentFileVO">
		INSERT INTO documen_file (documen_file_code, document_id,file_code) VALUES ( #{documenFileCode}, #{documentId}, #{fileCode})
	</insert>

	<delete id="doDelete" parameterType="DocumentFileVO">
		DELETE FROM documen_file WHERE file_code = #{fileCode}
	</delete>
	
	<select id="doSelectList"  parameterType="DocumentFileVO" resultType="DocumentFileVO">
		SELECT documen_file_code, document_id, file_code FROM documen_file WHERE document_id = #{documentId}
	</select>

</mapper>